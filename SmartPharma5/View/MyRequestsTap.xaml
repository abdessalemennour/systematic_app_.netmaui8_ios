<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SmartPharma5.View.MyRequestsTap"
             xmlns:local="clr-namespace:SmartPharma5.View"
            Title="My Requests"
     
        >

    <!--<TabbedPage.ToolbarItems>
        <ToolbarItem Text="Home" Order="Primary" Priority="1" IconImageSource="home.png" />

    </TabbedPage.ToolbarItems>-->


    <local:MyNotificationsProfilePartner  x:Name="first">
        <local:MyNotificationsProfilePartner.IconImageSource>
            <OnPlatform x:TypeArguments="ImageSource">
                <On Platform="Android" Value="update_temp.png" />
                <On Platform="iOS" Value="" />
            </OnPlatform>
        </local:MyNotificationsProfilePartner.IconImageSource>
    </local:MyNotificationsProfilePartner>
    <local:MyValidateUpdateOfProfile  x:Name="second" IsVisible="true">
        <local:MyValidateUpdateOfProfile.IconImageSource>
            <OnPlatform x:TypeArguments="ImageSource">
                <On Platform="Android" Value="update_temp.png" />
                <On Platform="iOS" Value="" />
            </OnPlatform>
        </local:MyValidateUpdateOfProfile.IconImageSource>
    </local:MyValidateUpdateOfProfile>
    <local:MyPartnerTempView  IsVisible="true"  x:Name="third">
        <local:MyPartnerTempView.IconImageSource>
            <OnPlatform x:TypeArguments="ImageSource">
                <On Platform="Android" Value="add_partner_temp.png" />
                <On Platform="iOS" Value="" />
            </OnPlatform>
        </local:MyPartnerTempView.IconImageSource>
    </local:MyPartnerTempView>
</TabbedPage>